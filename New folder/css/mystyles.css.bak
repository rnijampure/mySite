html, body {
    height:100%;
    margin:0;
    padding:0;
}
body {
    background: #e7e5d4 url(../images/bgSmall.png) right top fixed repeat;
}
.contentDiv {
    height:100%;
    width:100%;
    /*/flower.gif*/
    background: #e7e5d4 url(../images/very_big_women_bw_2.png) right top fixed no-repeat;
    font-family: Georgia, "Times New Roman", Times, serif;
    font-size:14px;
    color:#04026a;
    -moz-background-size: cover;
    -webkit-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    overflow:hidden;
}
.contentDiv:hover {
    height:100%;
    width:100%;
    background:url(../images/very_big_women_CLR.png) right top fixed no-repeat;
    -moz-background-size: cover;
    -webkit-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
}
h5 {
    font-size:12px;
    margin-left:5%;
}
h1 {
   background:url(../images/peacock.png) no-repeat -2px 25px transparent;
    line-height: 89px;
    padding-bottom: 29px;
    padding-left: 59px;
    line-height: 89px;
    padding-bottom: 29px;
    padding-left: 59px;
    color:#940f0f;
    font-family:"Monotype Corsiva";
    font-size:30px;
    margin-bottom:0;
}
#topNav {
    list-style-type: none;
    margin:0px;
    padding:0px;
    positon:fixed;
    top:0;
}
#topNav li {
    display:inline;
    padding:10px 20px 0px 10px;
    cursor:hand;
}
#slider {
    border-bottom:2px groove;
    position:absolute;
    height:2px;
    width:70px;
}
#homeCont {
    border:1px solid white;
    -moz-border-radius: 15px;
    border-radius: 15px;
    max-height:95%;
    height:85%;
    overflow: hidden;
    width:95%;
    margin-left:2%!important;
    background:rgba(255, 255, 255, 0.7);
    clear:both;
}
.hometext {
    font-size:15px;
    font-style:italic;
}
#sampleImgs img {
    border:5px groove #ffffff;
    margin:15px;
    float:left;
    padding:1px;
}

a {
    outline: none;
}

a img {
    border: 1px solid #BBB;
    padding: 2px;
    margin: 10px 20px 10px 0;
    vertical-align: top;
}

a img.last {
    margin-right: 0;    
}

ul {
    margin-bottom: 24px;
    padding-left: 30px; list-style-type:none;
}
#websiteDesignSamples{padding-left: 59px;}
#artWorkSamples{padding-left: 59px;}
#webSamples{float:left; width:100%;overflow-y:hidden; text-align:left;}